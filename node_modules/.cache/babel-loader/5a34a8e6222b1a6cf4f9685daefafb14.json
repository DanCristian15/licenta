\" />\n                <label for=\"geographicOrigin\">Geographic origin</label>\n                <v-text-field\n                    v-model=\"geographicOrigin\"\n                    :error-messages=\"[\n                        ...requiredError({ field: 'geographicOrigin' })\n\n                    ]\"\n                    placeholder=\"Geographic origin\" />\n                <label for=\"years\">Year/s</label>\n                <v-text-field\n                    v-model=\"years\"\n                    :error-messages=\"[\n                        ...requiredError({ field: 'years' })\n                    ]\"\n                    placeholder=\"Year/years\" />\n\n                <label>\n                    Start bid\n                    (All the prices will be condidered in USD)\n                </label>\n                <v-text-field\n                    v-model=\"startBid\"\n                    :error-messages=\"[\n                        ...requiredError({ field: 'startBid' })\n                    ]\"\n                    placeholder=\"Start bid\" />\n\n                <label for=\"categories\">Categories</label>\n                <v-select\n                    v-model=\"category\"\n                    :items=\"categories\"\n                    label=\"Choose a category\" />\n                <label for=\"description\">Description (optional) </label>\n                <v-textarea\n                    v-model=\"description\"\n                    placeholder=\"Description\"\n                    hint=\"Hint text\" />\n            </div>\n            <BaseButton block :placeholder=\"$t('subscribeButton')\" @click=\"validate\" />\n            <div> {{ getUsers() }} </div>\n        </v-form>\n    </div>\n</template>\n\n<script>\n    import { required } from 'vuelidate/lib/validators';\n    import { mapActions } from 'vuex';\n    import BaseButton from '@/core/shared/components/buttons/BaseButton.vue';\n    import { categories } from '@core/shared/mockdata/categories.js';\n\n    export default ( {\n        name: 'AddProduct',\n        components: {\n            BaseButton\n        },\n        data() {\n            return {\n                productName: '',\n                condition: '',\n                geographicOrigin: '',\n                years: '',\n                startBid: '',\n                category: '',\n                description: '',\n                categories: categories\n            };\n        },\n        validations( ) {\n            return {\n                productName: {\n                    required,\n                },\n                condition: {\n                    required,\n                },\n                geographicOrigin: {\n                    required,\n                },\n                years: {\n                    required,\n                },\n                startBid: {\n                    required,\n                },\n                category: {\n                    required,\n                }\n            };\n        },\n        methods: {\n            ...mapActions( {\n                notificationSuccess: 'notification/success',\n                commitSetProductName: 'addProduct/commitSetProductName',\n                commitSetCondition: 'addProduct/commitSetCondition',\n                commitSetGeographicOrigin: 'addProduct/commitSetGeographicOrigin',\n                commitSetYears: 'addProduct/commitSetYears',\n                commitSetStartBid: 'addProduct/commitSetStartBid',\n                commitSetCategory: 'addProduct/commitSetCategory',\n                commitSetDescription: 'addProduct/commitSetDescription'\n            } ),\n            // ...mapGetters( {\n            //     getUsers: 'user/signUp/getUsers'\n            // } ),\n            requiredError( { field } ) {\n                let errors = [];\n                if ( !this.$v[field].$dirty ) return errors;\n                !this.$v[field].required   && errors.push ( this.$t( '_common:formErrors.requiredField', { field: this.$t( `_common:form.labels.${field}` ), interpolation : { escapeValue: false } } ) );\n                return errors;\n            },\n            validate( ) {\n                this.$v.$touch( );\n\n                if ( !this.$v.$invalid ) {\n                    this.commitSetProductName( this.productName );\n                    this.commitSetGeographicOrigin( this.geographicOrigin );\n                    this.commitSetCondition( this.condition );\n                    this.commitSetYears( this.years );\n                    this.commitSetStartBid( this.startBid );\n                    this.commitSetCategory( this.category );\n                    this.commitSetDescription( this.description );\n                    //this.commitAddUser();\n                    // this.$router.push( { name: 'home' } );\n                    console.log( this.$data );\n                    this.notificationSuccess( this.$t( 'alerts.successfullyRegistered' ) );\n                }\n            },\n        }\n    } );\n</script>\n"],"sourceRoot":"src/core/views/addProduct"}]}