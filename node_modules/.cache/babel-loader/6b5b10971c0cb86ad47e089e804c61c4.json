          } );\n                }\n            },\n            'items.length': {\n                handler( ) {\n                    this.$nextTick( ( ) => {\n                        this.eliminateItem( );\n                    } );\n                }\n            }\n        },\n        methods: {\n            init( ) {\n                this.offset = this.items.length;\n                this.viewItems = [ ...this.items ];\n                this.$nextTick( ( ) => {\n                    this.eliminateItem( );\n                } );\n            },\n            eliminateItem( ) {\n                if ( this.$el && this.$el.scrollHeight > this.$el.offsetHeight && this.offset ) {\n                    this.offset = this.offset - 1;\n                    this.viewItems.pop( );\n                }\n            }\n        },\n        mounted( ) {\n            this.init( );\n            addListenerResize( this.$el, this.init );\n        },\n        destroyed( ) {\n            removeListenerResize( this.$el, this.init );\n        }\n    };\n</script>"],"sourceRoot":"src/core/shared/components/misc"}]}