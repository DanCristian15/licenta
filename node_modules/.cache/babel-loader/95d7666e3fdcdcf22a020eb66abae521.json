;AAIA,IAAA,WAJA,yBAIA;AACA,aAAA,KAAA,KAAA,CAAA,WAAA;AACA,KANA;AAOA,IAAA,aAPA,2BAOA;AACA,UAAA,KAAA,KAAA,CAAA,aAAA,EAAA;AACA,eAAA,KAAA,KAAA,CAAA,YAAA,GAAA,KAAA,QAAA,CAAA,OAAA,CAAA,IAAA,CAAA,KAAA,KAAA,CAAA,aAAA,CAAA,GAAA,EAAA;AACA;;AACA,aAAA,KAAA,EAAA,CAAA,yCAAA,CAAA;AACA,KAZA;AAaA,IAAA,iBAbA,+BAaA;AACA,aAAA,KAAA,KAAA,CAAA,WAAA,KAAA,cAAA,CAAA,YAAA,CAAA,SAAA;AACA,KAfA;AAgBA,IAAA,iBAhBA,+BAgBA;AAAA,wBACA,KAAA,KADA;AAAA,UACA,SADA,eACA,SADA;AAAA,UACA,WADA,eACA,WADA;AAAA,UACA,SADA,eACA,SADA;AAAA,UACA,UADA,eACA,UADA;AAAA,UACA,YADA,eACA,YADA;AAGA,aAAA;AACA,QAAA,IAAA,EAAA,SADA;AAEA,QAAA,MAAA,EAAA,WAFA;AAGA,QAAA,IAAA,EAAA,SAHA;AAIA,QAAA,SAAA,EAAA,UAJA;AAKA,QAAA,WAAA,EAAA;AALA,OAAA;AAOA,KA1BA;AA2BA,IAAA,kBA3BA,gCA2BA;AACA,aAAA,4BAAA,CAAA,KAAA,KAAA,CAAA;AACA,KA7BA;AA8BA,IAAA,QA9BA,sBA8BA;AAAA,UACA,KADA,GACA,IADA,CACA,KADA;AAEA,aAAA,KAAA,CAAA,QAAA;AACA,KAjCA;AAkCA,IAAA,eAlCA,6BAkCA;AAAA,UACA,KADA,GACA,IADA,CACA,KADA;AAEA,aAAA,KAAA,CAAA,eAAA;AACA;AArCA;AA1BA,CAAA","sourcesContent":["<template>\n    <v-card \n        flat \n        class=\"gst-event-card gst-event-card-variant-1 px-0\" \n        :class=\"{ 'gst-event-card--cancelled': isStatusCancelled }\"\n        :title=\"event.name\">\n        <v-layout align-center>\n            <v-flex d-flex>\n                <v-flex shrink class=\"d-flex align-start align-md-center\">\n                    <DateIcon outlined :start-date=\"event.startDateTime\" :end-date=\"event.endDateTime\" />\n                </v-flex>\n                <v-flex class=\"pl-4 xs12 d-flex flex-column align-md-center flex-md-row\">\n                    <v-flex \n                        :class=\"{\n                            'pr-0 pr-md-10': hasPriceIcon && hasTicketAvailable\n                        }\">\n                        <h5>\n                            <VClamp\n                                autoresize\n                                :max-lines=\"1\"\n                                class=\"gst-event-card-variant-1__title hyphens clamp\">\n                                {{ event.name }}\n                            </VClamp>\n                        </h5>\n                        <v-flex class=\"d-flex align-center\">\n                            <VenueInfo \n                                :max-lines=\"contentMaxLines\"\n                                class=\"gst-event-card-variant-1__venue gst-event-card__venue flex-grow-1 hyphens clamp mb-md-1\"\n                                :item=\"venueInfoLocation\">\n                                <template slot=\"before\">\n                                    <EventStatusChip \n                                        class=\"mr-1\" \n                                        :status=\"event.eventStatus\" />\n                                </template>\n                                <template v-if=\"eventDateTime\">\n                                    {{ eventDateTime }} &#183;\n                                </template>\n                            </VenueInfo>\n                        </v-flex>\n                    </v-flex>\n                    <v-flex v-if=\"hasPriceIcon && hasTicketAvailable\" shrink class=\"mt-3 mt-md-0\">\n                        <TenantPriceIcon\n                            class=\"d-inline-block\"\n                            :price=\"event.minPrice\" \n                            :points=\"offerPoints\" \n                            :currency=\"currency\"\n                            :loyalty-currency=\"loyaltyCurrency\"\n                            :is-loyalty-currency-type-money=\"isLoyaltyCurrencyTypeMoney\" \n                            :is-starting-price=\"event.minPrice !== event.maxPrice\" />\n                    </v-flex>\n                </v-flex>\n            </v-flex>\n        </v-layout>\n    </v-card>\n</template>\n<script>\n    import VClamp from 'vue-clamp';\n    import eventConstants from '@core/utils/constants/event';\n    import {\n        hasTicketAvailable as eventUtilsHasTicketAvailable\n    } from '@core/utils/eventUtils';\n    import DateIcon from '@core/shared/components/event/DateIcon.vue';\n    import EventStatusChip from '@core/shared/components/event/EventStatusChip.vue';\n    import VenueInfo from '@core/shared/components/containerInfo/VenueInfo.vue';\n\n    export default {\n        name: 'EventCardVariant1',\n        components: {\n            DateIcon,\n            VClamp,\n            EventStatusChip,\n            VenueInfo\n        },\n        props: {\n            event: {\n                type: Object,\n                required: true\n            },\n            withPriceIcon: {\n                type: Boolean,\n                default: false\n            },\n            isLoyaltyCurrencyTypeMoney: {\n                type: Boolean,\n                required: true\n            },\n            contentMaxLines: {\n                type: Number,\n                default: 0\n            }\n        },\n        computed: {\n            hasPriceIcon() {\n                return this.withPriceIcon;\n            },\n            offerPoints( ) {\n                return this.event.bonusPoints;\n            },\n            eventDateTime( ) {\n                if ( this.event.startDateTime ) {\n                    return this.event.hasStartTime ? this.$options.filters.time( this.event.startDateTime ) : '';\n                }\n                return this.$t( '_common:terms.dateAndTimeToBeDetermined' );\n            },\n            isStatusCancelled() {\n                return this.event.eventStatus === eventConstants.EVENT_STATUS.CANCELLED;\n            },\n            venueInfoLocation( ) {\n                const { venueName, venueStreet, venueCity, venueState, venueCountry } = this.event;\n\n                return {\n                    name:          venueName,\n                    street:        venueStreet,\n                    city:          venueCity,\n                    stateCode:     venueState,\n                    countryCode:   venueCountry\n                };\n            },\n            hasTicketAvailable( ) {\n                return eventUtilsHasTicketAvailable( this.event );\n            },\n            currency( ) {\n                const { event } = this;\n                return event.currency;\n            },\n            loyaltyCurrency( ) {\n                const { event } = this;\n                return event.loyaltyCurrency;\n            }\n        }\n    };\n</script>\n<style lang=\"scss\" scoped>\n@import \"@scssVariables\";\n@import \"@scssMixins\";\n\n.gst-event-card-variant-1 {\n    position: relative;\n    max-width: 100%;\n\n    .gst-event-card-variant-1__title {\n        font-weight: font-weight( 'medium' );\n        letter-spacing: -0.47px;\n    }\n\n    .gst-event-card-variant-1__price,\n    .gst-event-card-variant-1__venue {\n        color: theme-color( 'tertiary' );\n        font-size: font-size( 's' );\n    }\n\n    .gst-event-card-variant-1__price {\n        color: theme-color( 'tertiary' );\n        font-size: font-size( 's' );\n\n        .price {\n            color: theme-color( 'primary' );\n            font-weight: font-weight( 'bold' );\n        }\n    }\n}\n\n@include mobile-only {\n    .gst-event-card-variant-1 {\n        .gst-card-icon-container.gst-ticket-offer-price-icon {\n            width: auto;\n            min-width: 196px;\n        }\n    }\n}\n</style>\n"],"sourceRoot":"src/core/shared/components/event"}]}