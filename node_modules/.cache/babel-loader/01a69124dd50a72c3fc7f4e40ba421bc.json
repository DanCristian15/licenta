DA;AAEA,MAAA,OAAA,EAAA;AAFA;AAzBA,GATA;AAuCA,EAAA,QAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,UAAA,KAAA,0BAAA,EAAA;AACA,YAAA,QAAA,GAAA,KAAA,eAAA,IAAA,KAAA,QAAA;AACA,eAAA,KAAA,QAAA,CAAA,OAAA,CAAA,YAAA,CAAA,KAAA,MAAA,EAAA,QAAA,CAAA;AACA;;AAEA,aAAA,KAAA,QAAA,CAAA,OAAA,CAAA,WAAA,CAAA,KAAA,MAAA,CAAA;AACA,KARA;AASA,IAAA,WATA,yBASA;AACA,aAAA,KAAA,MAAA,GAAA,CAAA;AACA,KAXA;AAYA,IAAA,YAZA,0BAYA;AACA,aAAA,KAAA,MAAA,GAAA,CAAA;AACA,KAdA;AAeA,IAAA,WAfA,yBAeA;AAAA,UACA,cADA,GACA,IADA,CACA,cADA;AAEA,UAAA,kBAAA,GAAA,KAAA,EAAA,CACA,KAAA,0BAAA,GAAA,cAAA,GAAA,eADA,EAEA;AACA,QAAA,MAAA,EAAA,cADA;AAEA,QAAA,cAAA,EAAA,KAAA,cAFA;AAGA,QAAA,QAAA,EAAA,KAAA,eAAA,IAAA,KAAA;AAHA,OAFA,CAAA;;AAQA,UAAA,CAAA,KAAA,YAAA,EAAA;AACA,YAAA,KAAA,0BAAA,EAAA;AACA,iBAAA,EAAA;AACA;;AAEA,YAAA,UAAA,GAAA,KAAA,EAAA,CAAA,oBAAA,CAAA;AAEA,gIAEA,UAFA;AAIA;;AAEA,sHAEA,kBAFA;AAIA,KA1CA;AA2CA,IAAA,cA3CA,4BA2CA;AACA,UAAA,QAAA,GAAA,KAAA,eAAA,IAAA,KAAA,QAAA;AACA,aAAA,sBAAA,CAAA,QAAA,CAAA;AACA;AA9CA;AAvCA,CAAA","sourcesContent":["<template>\n    <CardIconContainer\n        :outlined=\"outlined\"\n        class=\"gst-ticket-offer-price-icon\"\n        flat>\n        <h5 class=\"gst-ticket-offer-price-icon__price\">\n            <template v-if=\"priceNumber\">\n                <span v-if=\"isStartingPrice\">{{ $t('_common:terms.from') }} </span>{{ price | currencyFull( currency ) }} <span>{{ $t( 'labels.priceSuffix' ) }}</span>\n            </template>\n            <template v-else>\n                {{ $t('labels.titleNoPrice') }}\n            </template>\n        </h5>\n        <!-- eslint-disable-next-line vue/no-v-html -->\n        <TenantBonusPointsChip\n            :points=\"points\"\n            :currency=\"loyaltyCurrency || currency\"\n            :is-loyalty-currency-type-money=\"isLoyaltyCurrencyTypeMoney\"\n            :content=\"contentChip\"\n            class=\"u-width-100 gst-ticket-offer-price-icon__bonus-points-chip\"\n            tag=\"div\" />\n    </CardIconContainer>\n</template>\n<script>\n    import { getSymbol as currencyUtilsGetSymbol  } from '@core/utils/currencyUtils';\n    import CardIconContainer from '@core/shared/components/misc/CardIconContainer.vue';\n\n    export default {\n        name: 'PriceIcon',\n        components: {\n            CardIconContainer,\n        },\n        i18nOptions: {\n            namespaces: 'shared',\n            keyPrefix: 'components.misc.priceIcon'\n        },\n        props: {\n            price: {\n                type: [ Number, String ],\n                default: 0\n            },\n            currency: {\n                type: String,\n                default: ''\n            },\n            loyaltyCurrency: {\n                type: String,\n                default: ''\n            },\n            points: {\n                type: [ Number, String ],\n                default: 0\n            },\n            isLoyaltyCurrencyTypeMoney: {\n                type: Boolean,\n                default: false\n            },\n            outlined: {\n                type: Boolean,\n                default: true\n            },\n            isStartingPrice: {\n                type: Boolean,\n                default: false\n            }\n        },\n        computed: {\n            pointsFormated( ) {\n                if ( this.isLoyaltyCurrencyTypeMoney ) {\n                    const currency = this.loyaltyCurrency || this.currency;\n                    return this.$options.filters.currencyFull( this.points, currency );\n                }\n\n                return this.$options.filters.bonusPoints( this.points );\n            },\n            priceNumber( ) {\n                return this.points * 1;\n            },\n            pointsNumber( ) {\n                return this.points * 1;\n            },\n            contentChip( ) {\n                const { pointsFormated } = this;\n                const contentBonusPoints = this.$t(\n                    this.isLoyaltyCurrencyTypeMoney ? 'labels.money' : 'labels.points',\n                    {\n                        points: pointsFormated,\n                        currencySymbol: this.currencySymbol,\n                        currency: this.loyaltyCurrency || this.currency\n                    }\n                );\n                if ( !this.pointsNumber ) {\n                    if ( this.isLoyaltyCurrencyTypeMoney ) {\n                        return '';\n                    }\n\n                    const titleChip0 = this.$t( 'labels.titleChip_0' );\n\n                    return `\n                        <p class=\"gst-ticket-offer-price-icon__bonus-points\">\n                            ${titleChip0}\n                        </p>`;\n                }\n\n                return `\n                    <p class=\"gst-ticket-offer-price-icon__bonus-points\">\n                        ${contentBonusPoints}\n                    </p>`;\n            },\n            currencySymbol( ) {\n                const currency = this.loyaltyCurrency || this.currency;\n                return currencyUtilsGetSymbol( currency );\n            }\n        }\n    };\n</script>\n<style lang=\"scss\" scoped>\n    @import \"@scssVariables\";\n\n    .gst-card-icon-container.gst-ticket-offer-price-icon {\n        width: 150px;\n\n        .gst-ticket-offer-price-icon__price {\n            line-height: 18px;\n            padding: 10px 5px 7px;\n            font-size: font-size( 's' );\n            font-weight: font-weight( 'bold' );\n\n            span {\n                font-size: font-size( 'xxs' );\n                font-weight: font-weight( 'regular' );\n            }\n        }\n\n        .gst-ticket-offer-price-icon__bonus-points-chip {\n            height: auto;\n            width: 100%;\n            line-height: line-height( 'xxs' );\n            padding: 5px 3px 4px;\n            font-size: font-size( 'xxxxs' );\n            font-weight: font-weight( 'bold' );\n            border-radius: border-radius( 'none' );\n            white-space: unset;\n            justify-content: center;\n            border-bottom-left-radius: inherit;\n            border-bottom-right-radius: inherit;\n        }\n    }\n</style>\n\n<style lang=\"scss\">\n    @import \"@scssVariables\";\n\n    .gst-card-icon-container.gst-ticket-offer-price-icon {\n        .gst-ticket-offer-price-icon__bonus-points-chip {\n            p {\n                margin-bottom: 0;\n            }\n\n            .gst-ticket-offer-price-icon__money {\n                text-transform: initial;\n\n                @media (min-width: 960px) {\n                    font-weight: font-weight( 'regular' );\n                }\n            }\n        }\n    }\n</style>\n"],"sourceRoot":"src/core/shared/components/misc"}]}