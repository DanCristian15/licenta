  :modal=\"item.modal\"\n                @close=\"remove( item.id )\" />\n        </div>\n    </div>\n</template>\n<script>\n    export default {\n        data() {\n            return {\n                modals: [ ]\n            };\n        },\n        methods: {\n            add( componentModal, modal ) {\n                let id = new Date( ).getTime( );\n\n                this.modals.push(\n                    {\n                        id,\n                        componentModal,\n                        modal\n                    }\n                );\n                return id;\n            },\n            remove( id ) {\n                const index = this.modals.findIndex( item => item.id === id );\n\n                if ( index !== -1 ) {\n                    this.modals.splice( index, 1 );\n                }\n\n                return this.modals;\n            }\n        },\n        created () {\n            this.$root._dynamicContainer = this;\n        },\n\n    };\n</script>\n"],"sourceRoot":"src/core/plugins/modal"}]}