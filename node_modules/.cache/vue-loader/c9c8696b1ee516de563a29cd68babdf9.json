leMusic from '@core/shared/assets/icons/apple_music.svg';\n\n    export default {\n        name: 'MusicLinks',\n        components: {\n            /* eslint-disable vue/no-unused-components */\n            IconSpotify,\n            IconAppleMusic\n            /* eslint-enable vue/no-unused-components */\n        },\n        i18nOptions: {\n            namespaces: 'main',\n            keyPrefix: 'views.attraction.theAttraction._components.musicLinks',\n        },\n        props: {\n            item: {\n                type: Object,\n                required: true\n            }\n        },\n        computed: {\n            buttons( ) {\n                const { item } = this;\n                return [\n                    {\n                        url: `https://open.spotify.com/search/${item.name}`,\n                        icon: 'IconSpotify'\n                    },\n                    {\n                        url: `https://music.apple.com/us/search?term=${item.name}`,\n                        icon: 'IconAppleMusic'\n                    }\n                ];\n            }\n        }\n    };\n</script>\n\n<style lang=\"scss\" scoped>\n    @import \"@scssVariables\";\n    @import \"@scssMixins\";\n    \n    .gst-attraction-music-links {\n        h6 {\n            line-height: line-height( 'xxl' );\n            font-size: font-size( 's' );\n            font-weight: font-weight( 'regular' );\n        }\n    }\n</style>"]}]}